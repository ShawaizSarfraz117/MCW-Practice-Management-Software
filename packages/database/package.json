{
  "name": "@mcw/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./mock": "./src/__mocks__/mockPrisma.ts",
    "./fabbrica": "./src/generated/fabbrica/index.ts",
    "./mock-data": "./src/__mocks__/data.ts"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "scripts": {
    "clean": "rm -rf dist",
