    "db:seed": "prisma db seed",
    "db:deploy": "prisma migrate deploy",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --pretty --checkJs",
    "format": "prettier --write src",
    "prettier:check": "prettier --check src"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@mcw/tsconfig-config": "*",
    "@quramy/prisma-fabbrica": "^2.2.2",
    "@types/node": "^20.11.17",
    "eslint": "^9.22.0",
    "prisma": "^6.6.0",
    "typescript": "^5.3.3"
  }
